  "translation": {
    "Message_0_Page_0": "Get away from me, <color=red>Red</color>!",
    "Message_1_Page_0": "You've been an exemplary <color=uncommon>Ensign</color>, and it's about time you were promoted to <color=rare>Leuitenant</color>!",
    "Message_2_Page_0": "How's it going, <name_char>?",
    "Message_3_Page_0": "Hello, you seem to be one person who will be a great <color=uncommon>Ensign</color> for the Coalition's cause. What's your name?",
    "Response_0": "My name is <name_char>. Yours?",
    "Response_1": "Thank you for the honor, sir. ",
    "Response_2": "Can I get a replacement uniform, sir?",
    "Response_3": "I mean no harm!",
    "Response_4": "It's good, sir."
  }
