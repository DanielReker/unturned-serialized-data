  "translation": {
    "Message_0_Page_0": "So you're <name_char>!  I gotta hand it to ya, you did quite the good job.",
    "Message_1_Page_0": "I can finally relax.  Though these nerves are gonna be steel regardless.",
    "Message_2_Page_0": "I get Sgt. Snuffy.  Buddy has a hard time moving around too.",
    "Message_3_Page_0": "I got no clue why the fuses for bombs are controlled by only three wires.",
    "Message_4_Page_0": "I could use a telescope sometime.",
    "Response_0": "Meet?",
    "Response_1": "Happy to meet you!",
    "Response_2": "Explosives Disposal",
    "Response_3": "Goodbye!"
  }
