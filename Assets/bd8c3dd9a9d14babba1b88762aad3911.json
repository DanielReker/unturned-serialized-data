  "translation": {
    "Message_0_Page_0": "Hey there! I'm <name_npc>. I help here at Ridders Van Licht HQ to bring the rest of the survivors to safety.",
    "Message_0_Page_1": "I came here after the meteor struck Ypres. <pause> That was a rough time.",
    "Message_0_Page_2": "Actually do you think you could do me a favor? I could really use some help.",
    "Message_1_Page_0": "Please hurry before anything else gets out of control.",
    "Message_2_Page_0": "You look like you've been pretty beat up. Did you take care of them all?",
    "Message_3_Page_0": "Thanks for everything. Be safe!",
    "Response_0": "Yeah what do you need me to do?",
    "Response_1": "Okay, I will take care of the zombies as fast as I can.",
    "Response_2": "I took care of all those fire zombies!",
    "Response_3": "Anytime!",
    "Response_4": "I don't really have the time right now. Talk to you later."
  }
