  "translation": {
    "Message_0_Page_0": "Uh, do I know you?",
    "Message_1_Page_0": "Don't mind me, just working on planting more crops.",
    "Message_2_Page_0": "Hey <name_char>, I'm almost finished planting this set of seedlings, only two more shelves left.",
    "Response_0": "Do you have some work I could do?",
    "Response_1": "I, uh, was wondering if I could try again?",
    "Response_2": "They grew up so fast.",
    "Response_3": "No, probably not.",
    "Response_4": "I'll let you get back to it.",
    "Response_5": "Keep up the hard work!"
  }
