  "translation": {
    "Message_0_Page_0": "Good day, <name_char>. I hope you are finding some comfort outside of the wastes.",
    "Message_1_Page_0": "Ah, <name_char> I presume, I've been expecting you.",
    "Message_2_Page_0": "<i>Uh.. Wha... How?!</i>",
    "Message_3_Page_0": "You'll regret this! You will see that you are on the wrong side!",
    "Response_0": "Stop right there, Criminal scum!",
    "Response_1": "You are under arrest!",
    "Response_2": "Your time is up, traitor.",
    "Response_3": "I guess we'll do this the old fashioned way.",
    "Response_4": "I highly doubt that.",
    "Response_5": "It's nice not having to watch my back."
  }
