  "translation": {
    "Message_0_Page_0": "Welcome to Ridders Van Licht HQ. I'm <name_npc>.",
    "Message_0_Page_1": "I need to stay focus, so no zombies can get in and infect us.",
    "Message_0_Page_2": "So could you give me a hand with something?",
    "Message_1_Page_0": "Hurry before anyone finds out.",
    "Message_2_Page_0": "Did you get what I asked?",
    "Message_3_Page_0": "We're in this together now, so I need you to do one more thing.",
    "Message_4_Page_0": "If you don't hurry they'll find out.",
    "Message_5_Page_0": "Did you get it?",
    "Message_6_Page_0": "Thanks, but if you say a word I will find you.",
    "Response_0": "What can I do to help?",
    "Response_1": "Alright, I'm going as fast as I can.",
    "Response_2": "Yes I got all of it.",
    "Response_3": "Fine, what else do you need?",
    "Response_4": "I got what you needed.",
    "Response_5": "Okay I understand; I won't say a word.",
    "Response_6": "I have to go."
  }
