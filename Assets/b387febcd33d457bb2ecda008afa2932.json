  "translation": {
    "Message_0_Page_0": "Alright, feeling better now?",
    "Message_1_Page_0": "Don't come crying to me if it gets worse then.",
    "Message_2_Page_0": "My work here is done. Looking good!",
    "Message_3_Page_0": "Why're you looking at me like that.",
    "Message_4_Page_0": "You're already doped up and healthy. Next patient!",
    "Message_5_Page_0": "Oh, come on.",
    "Message_6_Page_0": "You're lucky I can't do you any harm.",
    "Message_7_Page_0": "Just my luck. Healthy enough to come beg for medicine.",
    "Message_8_Page_0": "You're looking a bit green. Kidding. You do look awful though.",
    "Message_9_Page_0": "Bad day?",
    "Message_10_Page_0": "Look on the bright side, champ. You could look even worse.",
    "Message_11_Page_0": "Oh...- Nurse? NURSE!",
    "Message_12_Page_0": "Oh good grief if it isn't the Walking Turned! You look AWFUL!",
    "Message_13_Page_0": "Yuck. You got some hurt on me.",
    "Message_14_Page_0": "I've seen worse.",
    "Message_15_Page_0": "You are unturned.",
    "Response_0": "Infectious Care",
    "Response_2": "How much does infection care cost?",
    "Response_3": "Goodbye."
  }
}