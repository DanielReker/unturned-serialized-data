  "translation": {
    "Message_0_Page_0": "You may already know that this is Ridders Van Licht HQ and the operations conducted here.",
    "Message_0_Page_1": "We attempt to save as many survivors from this dreadful infection that has devoured Belgium.",
    "Message_0_Page_2": "Since you're here I'm going to put you to work!",
    "Message_1_Page_0": "DON'T JUST STAND AROUND! GO!",
    "Message_2_Page_0": "Did you find any Belgian Eclairs?",
    "Message_3_Page_0": "WE AREN'T DONE! THERE IS SO MUCH STUFF STILL TO BE DONE!",
    "Message_4_Page_0": "GET A MOVE ON!",
    "Message_5_Page_0": "What's your status?",
    "Message_6_Page_0": "Thank you soldier.",
    "Response_0": "I guess if you need my help.",
    "Response_1": "Yes sir!",
    "Response_2": "Yes, I found your Belgian Eclairs.",
    "Response_3": "Yes Sergeant I can do whatever else you ask!",
    "Response_4": "I did everything you asked, sir!",
    "Response_5": "You're wlecome, sir!",
    "Response_6": "I have to go. I have other duties elsewhere."
  }
