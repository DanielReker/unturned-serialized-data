  "translation": {
    "Message_0_Page_0": "Dag! I'm the chef here at Ridders Van Licht HQ. You can call me <name_npc>.",
    "Message_0_Page_1": "Actually if you aren't doing anything could you find me a couple ingredients?",
    "Message_1_Page_0": "I can't leave the oven on all day!",
    "Message_2_Page_0": "Could you find my ingredients?",
    "Message_3_Page_0": "Now I can make something delicious!",
    "Response_0": "What ingredients do you need?",
    "Response_1": "I will keep looking for the ingredients.",
    "Response_2": "I have your ingredients!",
    "Response_3": "No problem. Hopefully you're a good cook!",
    "Response_4": "I'll talk to you another time. I'm a little busy right now."
  }
