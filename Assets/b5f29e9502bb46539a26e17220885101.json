  "translation": {
    "Message_0_Page_0": "Hey you! Yeah I'm talking to you! I'm <name_npc>. ",
    "Message_0_Page_1": "I obviously fix stuff. I fix cars, jeeps, trucks. You name it, I fix it!",
    "Message_0_Page_2": "So I need you to get me some supplies and I'll fix you something right up!",
    "Message_1_Page_0": "Do you want a car or not?",
    "Message_2_Page_0": "It looks like you've been up to something kid. Do you have my supplies?",
    "Message_3_Page_0": "Where do you think you're going? I have more chores for you!",
    "Message_4_Page_0": "Let's get a move on then shall we!?",
    "Message_5_Page_0": "Look your back! Did you finish all my demands?",
    "Message_6_Page_0": "Thanks for all your hard work kid! Hopefully you make it out of this mess alive.",
    "Response_0": "Okay, what do I need to get you?",
    "Response_1": "Okay I'll hurry.",
    "Response_2": "I found your stuff you needed!",
    "Response_3": "I guess, what is it?",
    "Response_4": "Yes I did sir!",
    "Response_5": "Anytime! Yeah I hope the same for you!",
    "Response_6": "I'll just be on my way."
  }
