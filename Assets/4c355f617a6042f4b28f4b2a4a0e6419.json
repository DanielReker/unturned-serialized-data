  "translation": {
    "Message_0_Page_0": "Oh hello! I'm <name_npc>. I am one of the leads in this operation here at Ridders Van Licht HQ.",
    "Message_0_Page_1": "Our duty is to provide safety and security to the people of Belgium.",
    "Message_0_Page_2": "Would you like to help me and fulfill your duty?",
    "Message_1_Page_0": "You need to hurry so we can save as many people possible.",
    "Message_2_Page_0": "Are you done yet?",
    "Message_3_Page_0": "We thank you for helping Ridders Van Licht HQ. Be safe out there!",
    "Response_0": "How can I fulfill my duty?",
    "Response_1": "I'm working as fast as I can.",
    "Response_2": "I did what you asked!",
    "Response_3": "I'm glad to help.",
    "Response_4": "See you later. I have to do something."
  }
