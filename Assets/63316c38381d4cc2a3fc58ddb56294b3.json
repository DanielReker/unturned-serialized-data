  "translation": {
    "Message_0_Page_0": "The seas' calm today. Listen to those waves...",
    "Message_1_Page_0": "The sea calls out to my name. I can hear it.",
    "Message_2_Page_0": "Fishermen aren't always salty fellows. We keep the salt for the sea.",
    "Message_3_Page_0": "I absolutely hate those damned carp. ",
    "Response_0": "Need any help?",
    "Response_1": "I've made the better rod!",
    "Response_2": "Need any help?",
    "Response_3": "I've made the even better rod!",
    "Response_4": "Anything for trade?",
    "Response_5": "Goodbye."
  }
