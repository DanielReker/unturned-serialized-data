  "translation": {
    "Message_0_Page_0": "Greetings! You must be <name_char>, new survivors are a rare sight, word of your arrival spread quickly. I'm <color=epic><name_npc></color>, good to see a fresh face around here!",
    "Message_1_Page_0": "Anything new to report, <name_char>?",
    "Response_0": "Where'd you get the uniform?",
    "Response_1": "Thanks! I'll see you later.",
    "Response_2": "I'll report back later.",
    "Response_3": "Can I join the Coalition?",
    "Response_4": "You've been stranded?",
    "Response_5": "Need any assistance?",
    "Response_6": "I collected the munition crates.",
    "Response_7": "<color=uncommon>Chitchat</color> has found local military presence on the radio.",
    "Response_8": "How can we help them?",
    "Response_9": "Mission complete, sir!",
    "Response_10": "Mission complete, sir!",
    "Response_11": "Sir, Mayor Ekko has been infected."
  }
