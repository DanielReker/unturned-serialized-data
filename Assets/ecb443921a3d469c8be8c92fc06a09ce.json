  "translation": {
    "Message_0_Page_0": "Stay away, Coalition scum.",
    "Message_1_Page_0": "Hello, <color=rare>officer</color>.",
    "Message_2_Page_0": "You've been a great soldier. I officially promote you to <color=rare>Officer</color> of the <color=red>Irish Freedom Republic</color>.",
    "Message_3_Page_0": "Hello, <color=uncommon>recruit</color>.",
    "Message_4_Page_0": "Welcome to the <color=red>Irish Freedom Republic</color> base. I assume you would want to join, or are you one of them <color=blue>blue-bloods</color>?.",
    "Response_0": "Definitely! I'm <name_char>. Is there anything you need?",
    "Response_1": "I went to Dublin. It's not good.",
    "Response_2": "Do I have any missions, sir?",
    "Response_3": "I've got the armaments, sir.",
    "Response_4": "Thank you for the honor, sir.",
    "Response_5": "I just wanted to stop by, sir.",
    "Response_6": "I mean no harm!"
  }
