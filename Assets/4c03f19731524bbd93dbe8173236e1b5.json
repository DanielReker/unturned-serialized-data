  "translation": {
    "Message_0_Page_0": "You have been clearly making a name for yourself here <name_char>! I'm <name_npc> Lead Scientist for the Unyielding!",
    "Message_0_Page_1": "I have been studying this bio sample you brought back! I am beginning to think that Aegis might have something to do with the outbreak! I have a mission for you!",
    "Message_1_Page_0": "Now that you have found the camera, I've got a much more important matter for you to assist with.",
    "Message_2_Page_0": "Anything to report?",
    "Response_0": "I'll take a look",
    "Response_1": "Not right now",
    "Response_2": "I found the camera!",
    "Response_3": "I'll take a look",
    "Response_4": "Not right now",
    "Response_5": "I found that other bio sample!",
    "Response_6": "Nothing to report"
  }
