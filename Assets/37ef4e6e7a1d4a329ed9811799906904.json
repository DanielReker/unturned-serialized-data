  "translation": {
    "Message_0_Page_0": "Hey I'm <name_npc>.",
    "Message_0_Page_1": "Your're safe here. <pause> You don't have to worry anymore. We can fight this thing together!",
    "Message_0_Page_2": "I have so many things I have to do for these people here...",
    "Message_1_Page_0": "Okay please hurry though. This community is depending on me.",
    "Message_2_Page_0": "You're back sooner than I expected!",
    "Message_3_Page_0": "If you're not busy. I have another task for you.",
    "Message_4_Page_0": "There's not much time...",
    "Message_5_Page_0": "It looks like you've been up to something?",
    "Message_6_Page_0": "Thanks for the kindness in this age of dark times.",
    "Response_0": "Yeah it does seem pretty safe here. Is there something I can do to help?",
    "Response_1": "I have to go. I can't stay here.",
    "Response_2": "Alright, I won't let you down.",
    "Response_3": "I did all your tasks.",
    "Response_4": "What other tasks do you have for me?",
    "Response_5": "Okay, I will hurry.",
    "Response_6": "I finished with time to spare!",
    "Response_7": "Your welcome. <pause> I just hope everything will be back to normal someday."
  }
